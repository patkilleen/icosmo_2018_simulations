<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">
<properties>
   <comment>behavior of all the sensors that fail, they get more noisy (note the sensor entry is unused) </comment>
 
   <entry key="sensor-behavior.noise.config-path">input\fault-descriptions\fd_x_all_single_sensor_failures\global_behavior\white-noise.xml</entry>
   <entry key="sensor-behavior.amplification-factor">1</entry>
   <entry key="sensor-behavior.noise-amplification-factor">0</entry>
   <entry key="sensor-behavior.noise-pvalue-1">0.1</entry>
   <entry key="sensor-behavior.noise-pvalue-2">0.2</entry>
   <entry key="sensor-behavior.noise-type">MODIFY</entry>
   <entry key="sensor-behavior.sensor">null</entry>

</properties>